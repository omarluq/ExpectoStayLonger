    @font-face {
    font-family: "hp"
    src: asset-url("fonts/HARRYP__.TTF") format("truetype");
    }

    .session-div {
        z-index: 2000;
        width: 160%;
        height: 50%;
        position: fixed;
        top: 30%;
        left: 35%;
    }

      .session {
       display: flex;
       flex-direction: column;
       align-items: center;
       justify-content: space-around;
       border: 1px black solid;
       border-radius: 20px;
       background-color: white;
       max-width: 20%;
       background-color: white;
       padding: 5px;
       margin: 2px;
       flex-wrap: wrap;
   }

    .header{
        display: flex;
        align-content: center;
        justify-content: space-between;
        flex-wrap: wrap;
        <%# border-bottom: solid black thin; %>
        padding: 3px;
        background-color: transparent;
        z-index: 1;
    }

    .logOut{
        background-color: white;
        color: gray;
        padding: 20px;
        font-size: 16px;
        border: black thin solid;
        border-radius: 4px;
    }

    .img{
        background-image: url(<%= asset_path "landingpage.jpg" %>);
        width: 100vw;
        height: 90vh;
        background-size: cover;
        position: relative;
        top: -100px;
        z-index: -1;
    }


    .dropDownb{
        background-color: transparent;
        color: gray;
        padding: 6px;
        width: 80px;
        height: 40px;
        margin: 20px;
        font-size: 16px;
        border: black thin solid;
        border-radius: 25px;

    }

    .content{
        display: none;
        position: absolute;
        background-color: #f1f1f1;
        min-width: 160px;
        box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
        z-index: 1;
    }

    .dropDown{
        position: relative;
        display: inline-block;
    }

    .dropDown:hover .content {display: flex;
    flex-direction: column;}
    
 



   .logo {
       font-weight: 700;
       font-size: 25px;
       padding: 20px;
       font-family: hp;
   }