    @font-face {
    font-family: "hp"
    src: asset-url("fonts/HARRYP__.TTF") format("truetype");
    }

    .session-div {
        z-index: 2000;
        width: 160%;
        height: 50%;
        position: fixed;
        top: 30%;
        left: 35%;
    }

      .session {
       display: flex;
       flex-direction: column;
       align-items: center;
       justify-content: space-around;
       border: 1px black solid;
       border-radius: 20px;
       background-color: white;
       max-width: 20%;
       background-color: white;
       padding: 15px;
       margin: 2px;
       flex-wrap: wrap;
   }

    .header{
        display: flex;
        align-content: center;
        justify-content: space-between;
        flex-wrap: wrap;
        position: sticky;
        top: 0;
        background-color: transparent;
        z-index: 1;
    }

    .logOut{
        background-color: white;
        color: gray;
        padding: 20px;
        font-size: 16px;
        border: black thin solid;
        border-radius: 4px;
    }

    .img{
        background-image: url(<%= asset_path "landingpage.jpg" %>);
        width: 100vw;
        height: 90vh;
        background-size: cover;
        position: relative;
        top: -100px;
        z-index: -1;
        background-position-x: left;
        background-position-y: top;
    }



    h4 {
        font-weight: 700;
    }


    .dropDownb{
        background-color: transparent;
        color: gray;
        padding: 6px;
        width: 80px;
        height: 40px;
        margin: 25px;
        font-size: 16px;
        border: black thin solid;
        border-radius: 25px;
        color: black;
        

    }

    .content{
        display: none;
        position: absolute;
        background-color: #f1f1f1;
        min-width: 160px;
        box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
        z-index: 1;
    }

    .dropDown{
        position: relative;
        display: inline-block;
        cursor: pointer;
    }

    .dropDown:hover .content {
        padding-top: 15px;
        padding-bottom: 15px;
        display: flex;
        flex-direction: column;
        justify-content: space-around;
        position: fixed;
        right: 20px;
        top: 70px;
        border-radius: 6px;
        cursor: pointer;
    }

    .link:hover {
        background-color: #eae5e5;
    }

    
    
    .input{
        border-radius: 6px;
        padding: 6px;
        border: thin solid black;
        background-color: white;
        
    }



   .logo {
       display: flex;
       background-image: url(<%= asset_path "logo3.png" %>);
       background-repeat: no-repeat;
       width: 490px;
       height: 100px;
       z-index: 2; 
       margin-left: 25px;
  
   }