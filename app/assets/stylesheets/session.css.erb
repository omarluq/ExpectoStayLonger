    @font-face {
    font-family: "hp"
    src: asset-url("fonts/HARRYP__.TTF") format("truetype");
    }

    .session-div {
        z-index: 2000;
        width: 160%;
        height: 50%;
        position: fixed;
        top: 30%;
        left: 35%;
        display: flex;

    }

      .session {
       display: flex;
       flex-direction: column;
       align-items: center;
       border: 1px black solid;
       border-radius: 20px;
       background-color: white;
       width: 500px;
       background-color: white;
       flex-wrap: wrap;
   }

   .bottoms{
       margin-top: 10px;
       padding: 5px;
       border-top : 0.5px solid black;
       font-size: 12px;
   }


   .errors{
       font-size: 12px;
       list-style: 'Â¾ ' ;
   }

   .welcome {
       border-bottom: black 1px solid;
       padding: 20px;
       width: 20%%;
   }

   .sessionb {
       width: 350px;
       height: 40px;
   }

    .header{
        display: flex;
        align-content: center;
        justify-content: space-between;
        flex-wrap: wrap;
        position: sticky;
        top: 0;
        background-color: transparent;
        z-index: 1;
    }

    .logOut{
        background-color: white;
        color: gray;
        padding: 20px;
        font-size: 16px;
        border: black thin solid;
        border-radius: 4px;
    }

    .img{
        background-image: url(<%= asset_path "landingpage.jpg" %>);
        width: 100vw;
        height: 90vh;
        background-size: cover;
        position: relative;
        top: -100px;
        z-index: -1;
        background-position-x: left;
        background-position-y: top;
    }



    h4 {
        font-weight: 700;
    }


    .dropDownb{
        background-color: transparent;
        color: gray;
        padding: 6px;
        width: 80px;
        height: 40px;
        margin: 25px;
        font-size: 16px;
        border: black thin solid;
        border-radius: 25px;
        color: black;
        

    }

    .content{
        display: none;
        position: absolute;
        background-color: #f1f1f1;
        min-width: 160px;
        box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
        z-index: 1;
    }

    .dropDown{
        position: relative;
        display: inline-block;
        cursor: pointer;
    }

    .dropDown:hover .content {
        padding-top: 15px;
        padding-bottom: 15px;
        display: flex;
        flex-direction: column;
        justify-content: space-around;
        position: fixed;
        right: 20px;
        top: 70px;
        border-radius: 6px;
        cursor: pointer;
    }

    .link:hover {
        background-color: #eae5e5;
    }

    
    
    .input{
        border-radius: 6px;
        padding: 6px;
        border: thin solid black;
        background-color: white;
        
    }



   .logo {
       display: flex;
       background-image: url(<%= asset_path "logo3.png" %>);
       background-repeat: no-repeat;
       width: 490px;
       height: 100px;
       z-index: 2; 
       margin-left: 25px;
  
   }


    .search {
        margin-top: 20px;
        display: flex;
        justify-content: space-between;
        padding: 10px;
        border-radius: 25px;
        position: relative;
        right: 10%;
        background: transparent;
    }

    .search input {
        background-color: transparent;
        margin-top: 2px;
    }


    .search input:hover {
        border: 1px solid transparent;
        border-radius: 8px;
        background-color: #eae5e5;
    }

    .searchlabel {
        font-weight: 700;
        font-size: 12px;
    }

    .search button {
        background-color: transparent;
        border: 1px solid black;
        border-radius: 50%;
        padding: 1px;
        margin-left: 5px;
        width: 50px;
        height: 50px;
    }
    .date{
        padding: 3.5px;
         border-left:none;
        border-top:none;
        border-bottom: none;
        border-right :black solid 1px;
    }

    .inputs {
        padding: 6px;
        border: none;
    }

    .listings{
        display: flex;
        flex-direction: column;
    }

     .indlisting{
         padding: 20px;
         margin-left: 15px;
         display: flex;
         flex-direction: column;
         flex-wrap: wrap;
         border-top: 1px solid black;
         border-bottom: 1px solid black;
         max-width: 200px;
     }

     .stays {
         margin: 20px;
         display: flex;
         padding: 15px;
        flex-direction: column;
     }

     .wizarding{
        padding: 30px;
     }