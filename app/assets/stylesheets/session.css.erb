    }


   


   


    #mapContainer {
        position: fixed;
        right: 0;
        width: 50vw;
        height: 75vh;
        margin-top: 100px;
    }




    h4 {
        font-weight: 700;
    }


    .dropDownb{
        background-color: transparent;
        color: gray;
        padding: 6px;
        width: 80px;
        height: 40px;
        margin: 25px;
        font-size: 16px;
        border: black thin solid;
        border-radius: 25px;
        color: black;
        box-shadow: none;


    }

    .content{
        display: none;
        position: absolute;
        background-color: #f1f1f1;
        min-width: 160px;
        box-shadow: 0px 8px 16px 0px rgba(0,0,0 ,0.2);
        z-index: 1;
    }

    .dropDown{
        position: relative;
        display: inline-block;
        cursor: pointer;
    }

    .dropDown:hover .content {
        padding-top: 15px;
        padding-bottom: 15px;
        display: flex;
        flex-direction: column;
        justify-content: space-around;
        position: fixed;
        right: 20px;
        top: 70px;
        border-radius: 6px;
        cursor: pointer;
        box-shadow: 0 3px 20px rgba(0,0,0 , 0.5);
    }

    .link:hover {
        background-color: #eae5e5;
    }

    
    
    .input{
        border-radius: 6px;
        padding: 6px;
        border: thin solid black;
        background-color: white;
        
    }



   .logo {
       display: flex;
