    .header{
        display: flex;
        align-content: center;
        justify-content: space-between;
        position: sticky;
        top: 0;
        background-color: transparent;
        z-index: 1;
    }

    .on-scroll{
        display: flex;
        align-content: center;
        justify-content: space-between;
        position: sticky;
        top: 0;
        background-color: white;
        z-index: 1;
        box-shadow: 0 3px 20px rgba(0,0,0, 1);
    }



    .logOut{
        background-color: white;
        color: gray;
        padding: 20px;
        font-size: 16px;
        border: black thin solid;
        border-radius: 4px;
    }

    .img{
        background-image: url(<%= asset_path "landingpage.jpg" %>);
        width: 100vw;
        height: 90vh;
        background-size: cover;
        position: relative;
        top: -100px;
        z-index: -1;
